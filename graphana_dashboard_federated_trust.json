{
  "dashboard": {
    "title": "Federated Trust Dashboard",
    "panels": [
      {
        "title": "FTS Over Time",
        "type": "timeseries",
        "targets": [{ "expr": "rate(fts_metric[5m])" }],
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 }
      },
      {
        "title": "Audit Events",
        "type": "stat",
        "targets": [{ "expr": "audit_events_total" }],
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 }
      }
    ],
    "time": { "from": "now-6h", "to": "now" }
  },
  "folder": 0,
  "overwrite": false,
  "uid": "trust-dashboard"
}
